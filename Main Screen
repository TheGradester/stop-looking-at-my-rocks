from tkinter import *
from tkinter.ttk import *

#Button function
def clicked(*args):
    import gmaps
    import gmaps.datasets
    gmaps.configure(api_key='AIzaSyAqF7M212yx35SCQXiUeVqw9K5ltOz6H04')

    #locations
    Truckee = (39.327961,-120.183250)
    Incline = (39.250320,-119.961853)

    fig = gmaps.figure()
    Truckee2Incline = gmaps.directions_layer(
        Truckee, Incline, show_markers=False,
        stroke_weight=4.0, stroke_opacity=0.9)
    fig.add_layer(Truckee2Incline)
    fig
    
clicked()

# intializing the window
root = tk.Tk()
root.title("iRock")
root.configure(background = "lightgrey")
root.geometry('1920x1080')

#Create Tab Control
tabControl = ttk.Notebook(root)

#Tab1
tab1 = ttk.Frame(tabControl)
tabControl.add(tab1, text = 'My Feed')
tabControl.pack(expand = 1, fill = "both")

#Tab2
tab2 = ttk.Frame(tabControl)
tabControl.add(tab2, text = 'Map')
tabControl.pack(expand = 1, fill = "both")

#Tab3
tab3 = ttk.Frame(tabControl)
tabControl.add(tab3, text = 'Profile')
tabControl.pack(expand = 1, fill = "both")

#Adding button to tab2
btn1t2 = tk.Button(tab2, text="Map", command=clicked)
btn1t2.pack()

#Calling Main()

main_screen.mainloop()
main_account_screen()
